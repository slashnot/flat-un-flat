{
  "name": "flat-un-flat",
  "version": "0.0.4",
  "description": "A lightweight library to flatten and unflatten nested objects with separator or dot-notation keys",
  "author": "Ramkumar K",
  "email": "slashnot84@gmail.com",
  "homepage": "https://slashnot.github.io/flat-un-flat/",
  "repository": {
    "type": "git",
    "url": "https://github.com/slashnot/flat-un-flat.git"
  },
  "bugs": {
    "url": "https://github.com/slashnot/flat-un-flat/issues"
  },
  "keywords": [
    "flatten",
    "unflatten",
    "object",
    "nested",
    "dot-notation",
    "typescript"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./flatten": {
      "import": "./dist/flatten.js",
      "types": "./dist/flatten.d.ts"
    },
    "./unflatten": {
      "import": "./dist/unflatten.js",
      "types": "./dist/unflatten.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "predeploy": "pnpm run build",
    "deploy": "npx gh-pages -d dist",
    "preview": "vite preview"
  },
  "packageManager": "pnpm@10.23.0",
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "gh-pages": "^6.3.0"
  }
}